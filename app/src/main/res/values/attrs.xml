<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MenuDrawerContainer_">
        <attr name="collapsedMenuWidth" format="dimension" />
        <attr name="expandedMenuWidth" format="dimension" />
        <attr name="stateMenu" format="enum" >
            <enum name="open" value="0" />
            <enum name="close" value="1" />
            <enum name="hide" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MonthView">
        <attr name="colorBackground" format="color"/>
        <attr name="colorMonthName" format="color"/>
        <attr name="colorHeaderLine" format="color"/>
        <attr name="colorDayName" format="color"/>
        <attr name="colorWeekend" format="color"/>
        <attr name="colorDay" format="color"/>
        <attr name="colorSelectedDay" format="color"/>
        <attr name="textMonthNameSize" format="dimension"/>
        <attr name="textDayNameSize" format="dimension"/>
        <attr name="textDaySize" format="dimension"/>
        <attr name="visibleHeaderLine" format="boolean"/>
        <attr name="selectWeekend" format="boolean"/>
    </declare-styleable>

</resources>