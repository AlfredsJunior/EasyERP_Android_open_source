<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:id="@+id/flLeadItemContainer_LIL"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:background="@drawable/bg_with_bottom_divider_line_selector"
              android:orientation="vertical"
              android:padding="@dimen/list_item_24">


    <LinearLayout
        android:id="@+id/llFirstLine_VLIL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvLeadName_LIL"
                    style="@style/RobotoMedium"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text="@string/no_data"
                    android:textColor="@color/color_text_black"
                    android:textSize="@dimen/list_item_large_20"
                    tools:text="Web development"/>

                <TextView
                    android:id="@+id/tvPriority_LIL"
                    style="@style/RobotoMedium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:padding="@dimen/chips_padding"
                    android:text="@string/no_data"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/list_item_chips_large"
                    tools:text="Medium"
                    android:layout_marginRight="@dimen/list_item_chips_large"
                    android:layout_marginLeft="@dimen/list_item_side_8"/>

            </LinearLayout>

        </FrameLayout>

        <TextView
            android:id="@+id/tvStage_LIL"
            style="@style/RobotoRegular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="@string/no_data"
            android:textSize="@dimen/list_item_medium_16"
            tools:text="Not Interested"/>

    </LinearLayout>

</LinearLayout>


